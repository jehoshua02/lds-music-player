{
  "manifest_version": 2,
  "name": "LDS Music Player",
  "description": "Music player optimized for family routine.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "http://broadcast3.lds.org/crowdsource/Mobile/LDSMusic/Staging/Collections/*"
  ],
  "browser_action": {
    "default_icon": "extension/icon.png"
  },
  "background": {
    "scripts": ["extension/index.js"]
  }
}
