{
  "name": "lds-music-player",
  "version": "1.0.0",
  "description": "Music player optimized for family routine.",
  "main": "index.js",
  "scripts": {
    "init": "npm install",
    "watch": "npm run init; webpack -w --config webpack.watch.js",
    "test": "webpack --config webpack.test.js; mocha --opts ./mocha.opts --require should ./test/**/*.test.js",
    "build": "npm run init; webpack -p --config webpack.build.js",
    "clean": "rm -fr ./watch ./build ./node_modules",
    "deploy": "npm run clean; npm run build; gh-pages -d ./build"
  },
  "author": "Joshua Stoutenburg <jehoshua02@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jehoshua02/lds-music-player.git"
  },
  "bugs": {
    "url": "https://github.com/jehoshua02/lds-music-player/issues"
  },
  "homepage": "https://github.com/jehoshua02/lds-music-player#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "browser-sync": "^2.9.3",
    "browser-sync-webpack-plugin": "^1.0.0",
    "css-loader": "^0.19.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "gh-pages": "^0.4.0",
    "glob": "^5.0.15",
    "html-webpack-plugin": "^1.6.1",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.3",
    "node-sass": "^3.3.3",
    "sass-loader": "^2.0.1",
    "should": "^7.1.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.1",
    "webpack-notifier": "^1.2.1"
  },
  "dependencies": {
    "fuse.js": "^1.2.2",
    "random-int": "^1.0.0",
    "react": "^0.13.3"
  }
}
